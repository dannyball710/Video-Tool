{
  "manifest_version": 3,
  "name": "Video Tool",
  "description": "影片工具",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["video.js"],
      "css": ["style.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
